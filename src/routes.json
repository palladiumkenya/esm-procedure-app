        "meta": {
            "name": "completedTileSlot",
            "title": "Completed"
        }
    },
    {
        "name": "referred-tile-component",
        "slot": "procedure-tiles-slot",
        "component": "referredTileComponent",
        "meta": {
            "name": "referredTileSlot",
            "title": "Referred tests"
